<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <shippingfilters>
            <general>
                <enable>0</enable>
            </general>
            <country>
                <whitelist>US,CA,JP,RU,GB,FR</whitelist>
            </country>
            <region>
                <whitelist></whitelist>
            </region>
            <locality>
                <enable_locality_filters>0</enable_locality_filters>
                <auto_activate_postal_code>1</auto_activate_postal_code>
            </locality>
            <postal>
                <enable_postal_filters>0</enable_postal_filters>
                <auto_activate_locality>1</auto_activate_locality>
            </postal>
        </shippingfilters>
    </default>
</config>
